<div class="dashboard_sidebar">
  <div class="user-dashboard">
    <div class="card-deck" style="width: 65rem;">
      <% @products.each do |product| %>
        <% if current_user.id != product.user_id %> 
          <div class="card">
            <%= link_to image_tag(product.image, :class => "card-img-top"), dashboard_path(id: product.id) %>
            <div class="card-body" style="text-align: center">
              <h5> <%= product.name %> </h5>
              <p><%=link_to 'ADD TO CART',cart_items_path(id: product.id), method: :post, :class => "btn btn-info"%></p>
            </div>
          </div>
        <% end %>    
      <% end %>
    </div>
  </div>
</div>
