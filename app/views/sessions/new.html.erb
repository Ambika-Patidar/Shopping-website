        
      <%= form_with scope: :user, url: sessions_create_path, local: true do |f| %>
      <h2>
          Sign In
      </h2>
        <p>
            <%= f.label :email%>
            <%= f.text_field :email %><br>
        </p>
        <p>
            <%= f.label :password %>
            <%= f.password_field :password %><br>
        </p>

        <p>
            <%= f.submit 'Submit' %>  
        </p>
        <p>
            New user?<%= link_to 'Sign Up', registrations_new_path %>
        </p>
    <% end %>
